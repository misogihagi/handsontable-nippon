content13="
      var
        container2 = document.getElementById('example2'),
        hot2;

      hot3 = new Handsontable(container2, {
        data: Handsontable.helper.createSpreadsheetData(40, 6),
        colWidths: [47, 47, 47, 47, 47, 47, 47], // can also be a number or a function
        rowHeaders: true,
        colHeaders: true,
        stretchH: 'all',
        contextMenu: true
      });"
