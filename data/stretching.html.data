content8="
      var
        container1 = document.getElementById('example1'),
        hot1;

      hot1 = new Handsontable(container1, {
        data: Handsontable.helper.createSpreadsheetData(40, 6),
        colWidths: 47,
        rowHeaders: true,
        colHeaders: true,
        stretchH: 'last',
        contextMenu: true
      });"
